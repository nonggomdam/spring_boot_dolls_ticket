CREATE TABLE C##FIVEDOLLS.CUSTOMER_SERVICE_NOTICE_BOARD_M
(
    NOTICE_ID               VARCHAR2(15) NOT NULL,
    CUST_ID                 VARCHAR2(20),
    NOTICE_TITLE            VARCHAR2(80),
    NOTICE_DETAIL           VARCHAR2(500),
    NOTICE_CLASSIFICATION   VARCHAR2(15),
    NOTICE_KIND_CD          VARCHAR2(20),
    FIRST_CHANGE_DATE       DATE,
    LAST_CHANGE_DATE        DATE
)
TABLESPACE USERS
NOCOMPRESS;

ALTER TABLE C##FIVEDOLLS.CUSTOMER_SERVICE_NOTICE_BOARD_M
ADD PRIMARY KEY (NOTICE_ID);

ALTER TABLE C##FIVEDOLLS.CUSTOMER_SERVICE_NOTICE_BOARD_M
ADD CONSTRAINT CUSTOMER_SERVICE_NOTICE_BOARD_M_FK FOREIGN KEY (CUST_ID) REFERENCES C##FIVEDOLLS.MEMBER (CUST_ID);
