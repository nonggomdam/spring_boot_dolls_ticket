CREATE TABLE PERFORMANCE_SCHEDULE_D( -- 기존 테이블 컬럼 제약조건 수정
    PERFORMANCE_ID VARCHAR2(20)
    ,PERFORMANCE_LOCATION_ID VARCHAR2(20)
    ,PERFORMANCE_DATE DATE NOT NULL
    ,FIRST_CHANGE_DATE DATE NOT NULL
    ,LAST_CHANGE_DATE DATE NOT NULL
    ,CONSTRAINT PERFORMANCE_SCHEDULE_D_FK FOREIGN KEY (PERFORMANCE_ID) 
        REFERENCES PERFORMANCE_INFO_M(PERFORMANCE_ID)        
    ,CONSTRAINT PERFORMANCE_SCHEDULE_D_PK PRIMARY KEY (PERFORMANCE_ID,PERFORMANCE_LOCATION_ID,PERFORMANCE_DATE)    
);
